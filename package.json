{
  "name": "htmlstring-to-react",
  "version": "5.2.0",
  "description": "Convert HTML DOM to React elements",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc --project .",
    "clean": "rimraf lib/ yarn-error.log npm-debug.log",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "start": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomasthiebaud/htmlstring-to-react.git"
  },
  "author": "Thomas Thiebaud",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thomasthiebaud/htmlstring-to-react/issues"
  },
  "homepage": "https://github.com/thomasthiebaud/htmlstring-to-react#readme",
  "files": [
    "lib",
    "types"
  ],
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/git": "7.0.8",
    "@types/dompurify": "0.0.32",
    "@types/enzyme": "3.9.3",
    "@types/jest": "24.0.13",
    "@types/react": "16.8.18",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "conventional-changelog-eslint": "3.0.1",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.13.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-prettier": "3.1.0",
    "husky": "2.3.0",
    "jest": "24.8.0",
    "prettier": "1.17.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.27",
    "ts-jest": "24.0.2",
    "typescript": "3.4.5"
  },
  "dependencies": {
    "dompurify": "2.0.17"
  },
  "peerDependencies": {
    "react": "^16.2.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --extends @commitlint/config-conventional"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/tests/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>tests/setupTests.js"
    ]
  }
}
